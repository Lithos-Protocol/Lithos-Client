            "description" : "Array of UTXO ids"
          },
          "default" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ApiError"
                }
              }
            },
            "description" : "Error"
          }
        },
        "summary" : "List UTXO ids for blocks",
        "tags" : [ "blocks" ]
      }
    },
