          "propBytesHash" : "56002fb6194f8a7f147b54c98c98bd72205f46c35fed0f3c4111071ecb0763b7"
        },
        "properties" : {
          "propBytesHash" : {
            "description" : "Hexadecimal representation of hashed proposition bytes of the miner",
            "example" : "56002fb6194f8a7f147b54c98c98bd72205f46c35fed0f3c4111071ecb0763b7",
            "type" : "string"
          }
        },
        "required" : [ "propBytesHash" ],
        "type" : "object",
        "x-tags" : "blocks"
      },
