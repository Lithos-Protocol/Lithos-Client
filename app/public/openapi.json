          "in" : "query",
          "name" : "limit",
          "required" : false,
          "schema" : {
            "default" : 50,
            "format" : "int32",
            "maximum" : 100,
            "minimum" : 10,
            "type" : "integer"
          },
          "style" : "form"
        }, {
          "description" : "The offset to start the list from",
          "explode" : true,
          "in" : "query",
          "name" : "offset",
          "required" : false,
          "schema" : {
            "default" : 0,
            "format" : "int32",
            "type" : "integer"
          },
          "style" : "form"
        } ],
        "responses" : {
          "200" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "description" : "Array of UTXO ids",
                  "example" : [ "88c297f99da7172f6aa20efbead69aa40e2b7734a1873d66ca1db6b19d214db6" ],
                  "items" : {
                    "example" : "88c297f99da7172f6aa20efbead69aa40e2b7734a1873d66ca1db6b19d214db6",
                    "type" : "string"
                  },
                  "type" : "array"
