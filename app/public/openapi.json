          "blockId" : "2a7de28034ca523685d9a03c30d06c36863a804f69c2e4add4d83c8c2ba26271",
          "phase" : "HOLDING",
          "numMiners" : 12,
          "blockHeight" : 42853
        },
        "properties" : {
          "utxoId" : {
            "description" : "UTXO id of Lithos contract currently processing this block",
            "example" : "88c297f99da7172f6aa20efbead69aa40e2b7734a1873d66ca1db6b19d214db6",
            "type" : "string"
          },
          "blockId" : {
